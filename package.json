{
  "private": true,
  "name": "xo-server",
  "version": "0.0.0",
  "license": "ISC",
  "description": "",
  "keywords": [],
  "homepage": "",
  "bugs": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@isonoe.net"
  },
  "preferGlobal": true,
  "main": "dist/",
  "bin": {
    "xo-server": "dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "app-conf": "^0.4.0",
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.6",
    "event-to-promise": "^0.7.0",
    "http-request-plus": "0.0.0",
    "http-server-plus": "^0.6.4",
    "lodash": "^4.16.1",
    "promise-toolbox": "^0.5.1",
    "spdy": "^3.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-babili": "0.0.3",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "dependency-check": "^2.6.0",
    "ghooks": "^1.3.2",
    "jest": "^15.1.1",
    "standard": "^8.1.0"
  },
  "scripts": {
    "build": "NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "depcheck": "dependency-check ./package.json",
    "dev": "NODE_ENV=development babel --watch --source-maps --out-dir=dist/ src/",
    "lint": "standard",
    "posttest": "npm run lint && npm run depcheck",
    "prepublish": "npm run build",
    "start": "node dist/index.js"
  },
  "babel": {
    "env": {
      "production": {
        "presets": [
          "babili"
        ]
      }
    },
    "plugins": [
      "transform-decorators-legacy",
      "transform-runtime"
    ],
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  },
  "config": {
    "ghooks": {
      "commit-msg": "npm test"
    }
  }
}
